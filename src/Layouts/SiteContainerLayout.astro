---
import Navbar from "@components/Organisms/Navbar.astro";
import Footer from "@components/Organisms/Footer.astro";
import "@styles/global.scss";
export interface HeaderProps {
  title: string;
}
const { title = "Fenton Treasure | Portfolio" } = Astro.props as HeaderProps;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Cabin:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <Navbar />
    <section class="portfolio">
      <slot />
    </section>
    <!-- <Footer /> -->

  </body>
</html>
